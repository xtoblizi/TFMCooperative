<body>
    <!-- Navbar -->
    <nav class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a href="#" title="Tech Farmers Multipurpose Cooperative Society"> <br/>
                    <p> <span style="font-size: 3.0em; color: white; text-shadow: 4px 4px black; font-weight:bolder;"> TFMCooperative Society</span><br />
                        <span style="font-size: 1.2em; color: aliceblue"> Tech Farmers Multipurpose Cooperative Society</span>
                    </p><br />

                    @*<img class="logo"  src="~/FrontEnd/images/deslogo.png" alt="TFMCooperaitive" style="margin-top: 5px;" />*@
                </a>
            </div>
            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                <ul class="nav navbar-nav navbar-right" style="font-size: 1.2em; font-weight: bold; padding-top: 13px">
                    <li onmouseover="active"><a href="@Url.Action("Index","Home")">HOME</a></li>
                    <li onmouseover="active"><a href="@Url.Action("About","Home")">ABOUT US</a></li>

                    <li onmouseover="active"><a href="@Url.Action("Contact","Home")">CONTACT US</a></li>

                    <li onmouseover="active">@Html.Partial("_LoginPartial") </li>
                </ul>
            </div>
            <!-- /.navbar-collapse -->
        </div>
        <!-- /.container-fluid -->
    </nav>
<!-- Carousel -->

@*@Html.Partial("FrontEndPartials/_Carousel")*@

<!--Carousel End-->
@RenderBody()

    @Html.Partial("FrontEndPartials/_Footer")

<script src="~/FrontEnd/js/jquery.min.js"></script>
<script src="~/FrontEnd/bootstrap/js/bootstrap.min.js"></script>

    @RenderSection("scripts",required:false)
</body>
</html>
